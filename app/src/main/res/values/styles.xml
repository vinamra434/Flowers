<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="TextAppearance.Text.Default.HeadLine1" parent="TextAppearance.MaterialComponents.Headline1">
        <item name="android:textStyle">normal</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">@dimen/_16ssp</item>
        <item name="fontFamily">@font/oswald_regular</item>
        <item name="android:textColor">@color/black</item>
    </style>


    <style name="TextAppearance.Text.Splash.Large" parent="TextAppearance.Text.Default.HeadLine1">
        <item name="android:textSize">@dimen/_18ssp</item>
        <item name="android:textColor">@color/light_coral</item>
    </style>

    <style name="TextAppearance.Text.Item.Medium" parent="TextAppearance.Text.Default.HeadLine1">
        <item name="android:textSize">@dimen/_16ssp</item>
        <item name="fontFamily">@font/oswald_bold</item>
        <item name="android:textColor">@color/light_coral</item>
    </style>

    <style name="TextAppearance.Text.Item.Small" parent="TextAppearance.Text.Default.HeadLine1">
        <item name="android:textSize">@dimen/_14ssp</item>
        <item name="android:textColor">@color/bright_grey</item>
    </style>


</resources>